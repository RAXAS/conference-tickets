ticket_num = int(input("How many tickets?\n"))
total_price = 0
for i in range(ticket_num):
    age = int(input("How years old of visitor?\n"))
    if 18 <= age < 25:
        total_price += 990
    elif age < 18:
        total_price += 0
    elif age >= 25:
        total_price += 1390
if ticket_num > 3:
    total_price *= 0.9
print(f"Total price for {ticket_num} ticket(s)", int(total_price))
